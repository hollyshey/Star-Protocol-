import numpy as np

def hex_to_directions(hex_str):
directions = []
for i in range(0, len(hex_str), 2):
byte = int(hex_str[i:i+2], 16)
direction = byte % 6  # Ù¾Ù†Ø¬ Ø¬Ù‡Øª + Ø³Ú©ÙˆØª
directions.append(direction)
return directions

