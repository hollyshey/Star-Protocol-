import numpy as np

ÙØ±Ø¶ Ú©Ù†ÛŒÙ… Ù‡Ø± Ø¨Ù„Ø§Ú© Ø¨Ø§ ÙˆÛŒÚ˜Ú¯ÛŒâ€ŒÙ‡Ø§ÛŒ Ù…Ù‡Ù…Ø´ Ø¨Ù‡ Ø¨Ø±Ø¯Ø§Ø± 5 Ø¬Ù‡ØªÛŒ ØªØ¨Ø¯ÛŒÙ„ Ù…ÛŒØ´Ù‡

def encode_block_state(merkle_root, nonce, tx_count, fee, time_diff):
# Ù†Ù…ÙˆÙ†Ù‡ Ø³Ø§Ø¯Ù‡ Ø§Ø² Ú©Ø¯Ú¯Ø°Ø§Ø±ÛŒ Ûµ Ø¬Ù‡ØªÛŒ Ø¨Ø± Ø§Ø³Ø§Ø³ ØªØºÛŒÛŒØ±Ø§Øª
directions = []
for val in [merkle_root, nonce, tx_count, fee, time_diff]:
if val > 0.7:       directions.append('up')
elif val < 0.3:     directions.append('down')
else:               directions.append('steady')
return directions

Ù…Ø¯Ù„ Ø³Ø§Ø¯Ù‡ Ù¾ÛŒØ´â€ŒØ¨ÛŒÙ†ÛŒ Ø¨Ø± Ø§Ø³Ø§Ø³ Ûµ Ø¬Ù‡Øª Ùˆ Ù†Ù‚Ø§Ø· Ø¨Ø­Ø±Ø§Ù†ÛŒ

def predict_next_hash(current_state):
# Ø§Ú¯Ø± Ø¯Ø± Ù†Ù‚Ø§Ø· Ø¨Ø­Ø±Ø§Ù†ÛŒ Ù‡Ø³ØªÛŒÙ…ØŒ ØªØºÛŒÛŒØ±Ø§Øª Ø¨Ø²Ø±Ú¯ Ù¾ÛŒØ´â€ŒØ¨ÛŒÙ†ÛŒ Ú©Ù†ÛŒÙ…
if 'up' in current_state or 'down' in current_state:
predicted_change = np.random.choice(['up', 'down'])
else:
predicted_change = 'steady'
return predicted_change

